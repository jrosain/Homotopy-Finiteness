module Lib.Prop.Sg where

import Stdlib.Prelude

Sg-path/left (A : U) (B : A -> U) (u v : Sg A B) (p : Path (Sg A B) u v) : Path A u.1 v.1 =
  J (Sg A B) u (\ w _. Path A u.1 w.1) (refl A u.1) v p

